@model IEnumerable<egdbooking_v2.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink(Resources.Create, "AddIndex")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.OrderBy(u => u.Email)) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.ActionLink(Resources.Remove, "RemoveConfirm", new { id=item.Id })
        </td>
    </tr>
}

</table>
